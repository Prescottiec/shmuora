<!DOCTYPE html>
<html>
  <head>
    <title>Shmuora - A place to share knowledge and better understand the world.</title>
    <%= favicon_link_tag asset_path('favicon.png') %>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.home_logo = "<%= image_url('home.png') %>";
      window.github_logo = "<%= image_url('github.png') %>";
      window.linkedin_logo = "<%= image_url('linkedin.png') %>";
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
